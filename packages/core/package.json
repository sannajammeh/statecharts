{
  "name": "statecharts.sh",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.mts",
      "import": "./dist/schema/index.mjs"
    },
    "./schema.json": "./schema.json"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": ["dist", "schema.json"],
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@statecharts/typescript-config": "*",
    "tsdown": "^0.20.1",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
